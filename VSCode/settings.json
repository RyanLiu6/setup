{
    "workbench.startupEditor": "newUntitledFile",
    "files.autoSave": "onFocusChange",
    "[javascript]": {
        "editor.tabSize": 2
    },
    "[json]": {
        "editor.tabSize": 2
    },
    "[typescript]": {
        "editor.tabSize": 2
    },
    "telemetry.enableTelemetry": false,
    "telemetry.enableCrashReporter": false,
    "typescript.updateImportsOnFileMove.enabled": "always",
    "workbench.colorTheme": "Monokai Pro (Filter Octagon)",
    "workbench.iconTheme": "Monokai Pro (Filter Octagon) Icons",
    "editor.minimap.enabled": false,
    "editor.suggestSelection": "first",
    "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
    "files.trimTrailingWhitespace": true,
    "files.insertFinalNewline": true,
    "python.jediEnabled": false,
    "files.associations": {
        "Dockerfile_*":"dockerfile",
        "requirements_*":"pip-requirements",
        "*_requirements.txt":"pip-requirements"
    },
    "workbench.editorAssociations": {
        "*.ipynb": "jupyter-notebook"
    },
    "python.languageServer": "Default",
    "explorer.confirmDelete": false,
    "notebook.cellToolbarLocation": {
        "default": "right",
        "jupyter-notebook": "left"
    },
    "editor.tokenColorCustomizations": {
        "textMateRules": [
            {
                "scope": "string.quoted.docstring.multi.python",
                "settings": {
                    "foreground": "#EEBA30"
                }
            },
            {
                "scope": "string.quoted.docstring.multi.python punctuation.definition.string.end.python",
                "settings": {
                    "foreground": "#EEBA30"
                }
            },
            {
                "scope": "string.quoted.docstring.multi.python punctuation.definition.string.begin.python",
                "settings": {
                    "foreground": "#EEBA30"
                }
            }
        ]
    },
    "autoDocstring.startOnNewLine": true
}
